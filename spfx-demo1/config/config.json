{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "hello-world-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/helloWorld/HelloWorldWebPart.js",
          "manifest": "./src/webparts/helloWorld/HelloWorldWebPart.manifest.json"
        }
      ]
    },
    "e-shop-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/eShop/EShopWebPart.js",
          "manifest": "./src/webparts/eShop/EShopWebPart.manifest.json"
        }
      ]
    },
    "urun-siparis-takip-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/urunSiparisTakip/UrunSiparisTakipWebPart.js",
          "manifest": "./src/webparts/urunSiparisTakip/UrunSiparisTakipWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "HelloWorldWebPartStrings": "lib/webparts/helloWorld/loc/{locale}.js",
    "EShopWebPartStrings": "lib/webparts/eShop/loc/{locale}.js",
    "UrunSiparisTakipWebPartStrings": "lib/webparts/urunSiparisTakip/loc/{locale}.js",
    "ControlStrings": "node_modules/@pnp/spfx-controls-react/lib/loc/{locale}.js"
  }
}